{
  "compilerOptions": {
      "outDir": "./built/",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "esnext",
      "target": "es6",
      "jsx": "preserve",
      "moduleResolution": "node" // if don't set this, webpack won't work
  },
  "include": [
      "./src"
  ],
  "exclude": [
      "/node_modules",
      "./test"
  ]
}
